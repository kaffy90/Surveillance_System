import RPi.GPIO as GPIO
import time

GPIO.setmode(GPIO.BCM)
GPIO.setwarnings(False)
GPIO.output(21, GPIO.OUT)
try:
	while True:
		print 'LED ON'
		GPIO.output(21, GPIO.HIGH)
		time.sleep(2)
		print 'LED OFF'
		GPIO.output(21, GPIO.LOW)
		time.sleep(1)
except KeyboardInterrupt:
		GPIO.output(21, GPIO.LOW)
		GPIO.cleanup()